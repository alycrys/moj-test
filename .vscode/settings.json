{
  "prettier.eslintIntegration": true,
  "prettier.singleQuote": true,
  "eslint.options": {
    "parserOptions": {
      "ecmaVersion": 6,
    }
  },
  "eslint.enable": true,
  "editor.renderWhitespace": "all",
  "editor.trimAutoWhitespace": true,
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave"
}
